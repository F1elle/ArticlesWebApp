{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DbConnection": "Data Source=db/Articles.db"
  },
  "JwtOptions": {
    "SecretKey": "VerySecretKeyDontStealItPleaseLetItBeSecret",
    "ExpiresHours": "16"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.SQLite"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  }
}
